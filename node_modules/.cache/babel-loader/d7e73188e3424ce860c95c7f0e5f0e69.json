A,IAAA,CAAA;AADA,WADA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,MAAA;AACA,WANA,CAAA;AAOA,SAZA,EAaA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,OAAA;AACA,SAfA;AAkBA,QAAA,QAAA,CAAA,IAAA,GAAA,kBAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,EAAA;AACA,gBAAA,GAAA,GAAA,IAAA,CAAA,GAAA;AAEA,YAAA,EAAA,CAAA,UAAA,CAAA,MAAA,EACA,GADA,CACA,GADA,EAEA,GAFA,CAEA;AACA,cAAA,IAAA,EAAA,IAAA,CAAA,WADA;AAEA,cAAA,KAAA,EAAA,IAAA,CAAA,KAFA;AAGA,cAAA,GAAA,EAAA;AAHA,aAFA;AAOA;AACA,SAZA;AAaA;AACA;AAxCA,GATA;AAmDA,EAAA,KAAA,EAAA;AACA,IAAA,OAAA,EAAA,mBAAA;AACA,UACA,KAAA,OAAA,KAAA,EAAA,IACA,KAAA,OAAA,KAAA,EADA,IAEA,KAAA,OAAA,KAAA,KAAA,OAHA,EAIA;AACA,aAAA,KAAA,GAAA,iCAAA;AACA,OANA,MAMA;AACA,aAAA,KAAA,GAAA,IAAA;AACA;AACA;AAXA;AAnDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <form class=\"mt-5\" @submit.prevent=\"register\">\r\n      <div class=\"container\">\r\n        <h4 class=\"text-center font-weight-light mb-3\">Registrate como entidad</h4>\r\n        <div class=\"form-row\">\r\n          <div v-if=\"error\" class=\"col-12 alert alert-danger px-3\">{{ error }}</div>\r\n          <section class=\"mt-5 col-sm-12 form-group\">\r\n            <label class=\"form-control-label sr-only\" for=\"displayName\">Display Name</label>\r\n            <input\r\n              class=\"form-control\"\r\n              type=\"text\"\r\n              id=\"displayName\"\r\n              placeholder=\"Nombres y apellidos\"\r\n              name=\"displayName\"\r\n              required\r\n              v-model=\"displayName\"\r\n            />\r\n          </section>\r\n        </div>\r\n        <section class=\"form-group\">\r\n          <label class=\"form-control-label sr-only\" for=\"email\">Email</label>\r\n          <input\r\n            class=\"form-control\"\r\n            type=\"email\"\r\n            id=\"email\"\r\n            placeholder=\"Correo electr칩nico\"\r\n            required\r\n            name=\"email\"\r\n            v-model=\"email\"\r\n          />\r\n        </section>\r\n        <section class=\"form-group\">\r\n          <label class=\"form-control-label sr-only\"></label>\r\n          <input class=\"form-control\" type=\"password\" placeholder=\"Contrase침a\" v-model=\"passOne\" />\r\n        </section>\r\n        <section class=\"form-group\">\r\n          <label class=\"form-control-label sr-only\"></label>\r\n          <input\r\n            class=\"form-control\"\r\n            type=\"password\"\r\n            required\r\n            placeholder=\"Repetir contrase침a\"\r\n            v-model=\"passTwo\"\r\n          />\r\n        </section>\r\n\r\n        <div class=\"form-group text-center mt-5\">\r\n          <button class=\"btn btn-primary\" type=\"submit\">Registrarse</button>\r\n        </div>\r\n      </div>\r\n    </form>\r\n    <p class=\"text-center mt-2\">\r\n      o\r\n      <router-link to=\"/logincliente\">Ingresar</router-link>\r\n    </p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// eslint-disable-next-line\r\nimport Firebase from \"firebase\";\r\nimport db from \"../db.js\";\r\n\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      displayName: null,\r\n      email: null,\r\n      passTwo: null,\r\n      error: null\r\n    };\r\n  },\r\n  methods: {\r\n    register: function() {\r\n      const info = {\r\n        email: this.email,\r\n        password: this.passOne,\r\n        displayName: this.displayName\r\n      };\r\n      if (!this.error) {\r\n        Firebase.auth()\r\n          .createUserWithEmailAndPassword(info.email, info.password)\r\n          .then(\r\n            // eslint-disable-next-line\r\n            userCredentials => {\r\n              return userCredentials.user\r\n                .updateProfile({\r\n                  displayName: info.displayName\r\n                })\r\n                .then(() => {\r\n                  this.$router.replace(\"home\");\r\n                });\r\n            },\r\n            error => {\r\n              this.error = error.message;\r\n            }\r\n          );\r\n\r\n      Firebase.auth().onAuthStateChanged(user => {\r\n          if (user) {\r\n            var UID = user.uid;\r\n\r\n            db.collection(\"user\")\r\n              .doc(UID)\r\n              .set({\r\n                name: info.displayName,\r\n                email: info.email,\r\n                uid: UID\r\n              });\r\n          }\r\n        });\r\n      }\r\n    }\r\n  },\r\n  watch: {\r\n    passTwo: function() {\r\n      if (\r\n        this.passOne !== \"\" &&\r\n        this.passTwo !== \"\" &&\r\n        this.passTwo !== this.passOne\r\n      ) {\r\n        this.error = \"Las contrase침as deben coincidir\";\r\n      } else {\r\n        this.error = null;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.container {\r\n  margin-top: 8%;\r\n  padding-left: 200px;\r\n  padding-right: 200px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}